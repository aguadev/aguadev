.view .leftPane .projectTitle, .view .leftPane .workflowTitle,
.view .leftPane .reportTitle, .view .leftPane .fileTitle,
.view .leftPane .viewTitle {
	text-align: center;
	font-size: 14px;
	font-weight: bolder;
	font-family: Verdana;
	font-style: oblique;
	height: 20px;
	vertical-align: bottom;
	background: #EEE;
}

.view .leftPane .message {
	color: #444;
	font-family: Verdana;
	font-size: 12px;
	font-style: italic;
	text-align: center;
	padding-right: 35px;
}


.view .leftPane .fileIcon, .view .leftPane .reportIcon,
.view .leftPane .viewIcon, .view .leftPane .workflowIcon,
.view .leftPane .projectIcon {
	background-repeat: no-repeat;
	background-position: center;
	min-width: 35px;
	min-height: 35px;
	width: 35px;
	height: 35px;
}

.view .leftPane .fileIcon {
	background-image: url("../../../plugins/view/images/file-28.png");
}
.view .leftPane .reportIcon {
	background-image: url("../../../plugins/view/images/report-28.png");
}
.view .leftPane .viewIcon {
	background-image: url("../../../plugins/view/images/view-28.png");
}
.view .leftPane .workflowIcon {
	background-image: url("../../../plugins/view/images/workflow-28.png");
}
.view .leftPane .projectIcon {
	background-image: url("../../../plugins/view/images/project-28.png");
}


.view .leftPane .saveViewButton, .view .leftPane .deleteViewButton,
.view .leftPane .saveReportButton,  .view .leftPane .deleteReportButton,
.view .leftPane .okButton {
	background-color: #FFF;
	border: 2px solid #EEE;
	border-bottom-color: #DDD;	
	border-left-color: #DDD;	
	vertical-align: middle;
	background-position: center;
	background-repeat: no-repeat;
	width: 28px;
	height: 28px;
}

.view .leftPane .saveViewButton {
	background-image: url("../../../plugins/view/images/save-24.png");
}
.view .leftPane .deleteViewButton {
	background-image: url("../../../plugins/view/images/delete-24.png");
}
.view .leftPane .saveReportButton {
	background-image: url("../../../plugins/view/images/save-24.png");
}
.view .leftPane .deleteReportButton {
	background-image: url("../../../plugins/view/images/delete-24.png");
}
.view .leftPane .okButton {
	background-image: url("../../../plugins/view/images/OK-34.png");
}

.view .leftPane .saveReportButton:hover, .view .leftPane .fileUpload:hover,
.view .leftPane .fileBrowse:hover, .view .leftPane .deleteReportButton:hover,
.view .leftPane .okButton:hover {
	border-bottom-color: #BBB;	
	border-left-color: #BBB;
	background-color : #FF8;
}

.view .leftPane .saveReportButton:active, .view .leftPane .fileUpload:active,
.view .leftPane .fileBrowse:active, .view .leftPane .deleteReportButton:active,
.view .leftPane .okButton:active {
	border-bottom-color: #BBB;	
	border-left-color: #BBB;
	background-color : #8F8;
}


.view .leftPane .completed {
	background-image: url("../../../plugins/view/images/completed-18.png");
	background-repeat: no-repeat;
	background-position: center;
	min-width: 35px;
	min-height: 35px;
	width: 35px;
	height: 35px;
}

.view .leftPane .loading {
	background-image: url("../../../plugins/view/images/loading-32.gif") !important;
	background-repeat: no-repeat;
	background-position: center;
	min-width: 35px;
	min-height: 35px;
	width: 35px;
	height: 35px;
}




.view .leftPane .fileBrowse, .view .leftPane .fileUpload {
	background-color: #FFF;
	border: 2px solid #EEE;
	border-bottom-color: #DDD;	
	border-left-color: #DDD;	
	vertical-align: middle;
	float: inherit;
	min-width: 28px;
	background-position: center;
	background-repeat: no-repeat;
	width: 28px;
	height: 28px;
}

.view .leftPane .fileBrowse td:hover, .view .leftPane .fileUpload td:hover {
	background-image: url();
	color: #F00;
	border: 2px solid #FFD;
	border-bottom-color: #EEC;	
	border-left-color: #EEC;
	background-color : #0F0;
}

.view .leftPane .fileBrowse {
	background-image: url("../../../plugins/view/images/browse-28.png");
}
.view .leftPane .fileUpload {
	background-image: url("../../../plugins/view/images/upload-28.png");
}

/* EXPOSE FILE UPLOAD ONCLICK ELEMENT */
.view .leftPane .dijitFileInput {
	position: relative;
	top: -2px;
	left: -3px;
	height: 32px !important;
	width: 32px !important;
	z-index: 10;
	border: 0px;
}

.view .leftPane .dijitFileInputReal {
	background-color: #00F !important;
	height: 30px !important;
	width: 30px !important;
	z-index: 11;
}

/* REMOVE BLANK AT RIGHT OF COMBO INPUT */
.view .leftPane .dijitReset .dijitValidationIcon {
	visibility: hidden;
	display: none;
}

.view .leftPane .projectCombo, .view .leftPane .workflowCombo,
.view .leftPane .reportCombo, .view .leftPane .viewCombo {
    font-family: sans-serif;
    font-size: 18px;
    width: 130px;
	text-indent: 2px;
	/*color: #F00;*/
    /*size: 250px;*/
    /*max-width: 30px;*/
    /*line-height: 50px !important;*/
}



.view .leftPaneTabContainer {
	background-color: #000;
}


/* MIDDLE PANE */


.view .middlePane div {
	display: inline-block !important;
	position: relative;
}

.view .middlePane {
	background-color: #000;
}


.browserContainer {	
	height:	900px;
}


.view .dijitLayoutContainer {
	top: 0px;
	left: 0px;
}

